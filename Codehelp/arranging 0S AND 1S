#include<iostream>
using namespace std;


int main(){
    
    int arr[] = {1,1,1,0,1,0,0,0,1,1};
    int n = sizeof(arr)/sizeof(arr[0]);
    int start = 0;
    int end = n-1;
    
    for(int i = 0; i<n; i++){
        
        if(arr[i] == 0){
            start++;
        }
        
        else if(arr[i] == 1){
            end--;
        }
        
        else{
            swap(arr[start], arr[end]);
            start++;
            end--;
        }
        
    }
    
    
    for(int i = 0; i<n; i++){
        cout<<arr[i]<<" ";
    }
    
    
}

