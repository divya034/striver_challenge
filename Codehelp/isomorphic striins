class Solution {
public:
    bool isIsomorphic(string s, string t) {
        int hash[255]= {0};
        bool isttaken[255] = {0};

        for(int i = 0; i<s.size(); i++){
          if(hash[s[i]] == 0 && isttaken[t[i]] == 0)  
            hash[s[i]] = t[i];
            isttaken[t[i]] = true;
        }

        for(int i = 0; i<s.size(); i++){
            if(hash[s[i]] != t[i]){
                return false;
            }
        }

        return true;
    }
};
