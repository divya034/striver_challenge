
#include <iostream>
#include<string.h>
using namespace std;

int findlen(char name[]){
    
    int len = 0;
    
    for(int i = 0; name[i] !='\0'; i++){
        len++;
    }
    
    return len;
}

void reversestr(char name [], int n){
    
    int i = 0, j = n-1;
    
    while(i < j){
        
        swap(name[i++], name[j--]);
        
    }
}


void replacespace(char name[]){
    
    int n = strlen(name);
    int i = 0;
    
    while(i < n){
        
        if(name[i] == ' '){
            name[i] = '@';
        }
        
        i++;
    }
}


bool checkpalindrone(char str[]){
    
    int i = 0, j = strlen(str) - 1;
    
    while(i<=j){
        if(str[i] != str[j]){
            return false;
        }
        
      i++;j--;
    }
    
    return true;
    
}

void converttoupper(char str[]){
    
    int n = strlen(str);
    
    for(int i = 0; i<n; i++){
        if(str[i] >= 'a' && str[i] <= 'z') str[i] = str[i] - 'a' + 'A';
    }
    
}

int main()
{
