//   reversestr(name, n);
//   cout<<name;

//   char name[30];
   
//   cout<<"enter your name \n";
//   cin.getline(name, 30);
  
   
//   replacespace(name);
   
//   cout<<name;

// char name[50] = "div";

// cout<<checkpalindrone(name);

  char name[30];
  
  cin.getline(name, 30);
  
  converttoupper(name);

  cout<<name;


}
